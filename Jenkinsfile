 pipeline{

    agent any

   stages{
    stage('build'){
            steps{
                sh 'echo build'
            }
        }
        stage('test'){
            steps{
                sh 'echo test'
            }
        }
        stage('pwd command'){
            steps{
                sh 'pwd'
            }
        }
        stage('ls command'){
            steps{
                sh 'ls'
            }
        }
        
    }
}