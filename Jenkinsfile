 pipeline{

    agent any

   stages{
    stage('build'){
            steps{
                sh 'echo build'
            }
        }
        stage('pwd command'){
            steps{
                sh 'pwd'
            }
        }
        stage('ls command'){
            steps{
                sh 'ls'
            }
        }
        stage('whoami command'){
            steps{
                sh 'whoami'
            }
        }
    }
}